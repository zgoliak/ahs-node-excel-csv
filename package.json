{
 "name": "nodejs-image-demo",
 "version": "1.0.0",
 "description": "AHS Docker with Node JS generating Excel and CSV files from Salesforce data and pushing to SFTP on a CRON schedule",
 "author": "Zakee Goliak",
 "license": "UNLICENSED",
 "main": "app.js",
 "keyword": [
  "nodejs",
  "express",
  "jsforce",
  "node-cron",
  "xlsx",
  "fast-csv",
  "ssh2"
 ],
 "scripts": {
  "test": "node ./app.js test",
  "start": "node ./app.js",
  "build-dock": "docker build -t ahs/docker_node_cron_ssh_xlsx_csv:1.0 .",
  "start-dock": "docker run -d ahs/docker_node_cron_ssh_xlsx_csv:1.0"
 },
 "dependencies": {
  "express": "^4.16.4",
  "node-cron": "2.0.3",
  "jsforce": "1.9.3",
  "xlsx": "0.15.5",
  "fast-csv": "4.0.2",
  "ssh2": "0.8.7",
  "excel-compare": "0.1.3"
 }
}
